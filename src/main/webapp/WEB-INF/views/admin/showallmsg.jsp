<%@ page import="java.util.List" %><%@ page import="com.jisu.emp_management.model.AdminMsg" %><!doctype html><html lang="en"><head>    <!-- Required meta tags -->    <meta charset="utf-8">    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">    <!-- Bootstrap CSS -->    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">    <title>All messages</title></head><body style="background-color: black;"><%   List<AdminMsg> messages=(List<AdminMsg>) request.getAttribute("messages");%><div class="container text-center">    <h2 style="color: green;"> All public messages</h2></div><div class="container-fluid">    <table class="table table-dark">        <thead class="table-info">        <tr style="color: violet;">            <th scope="col">Name</th>            <th scope="col">Email</th>            <th scope="col">Message</th>            <th scope="col">Action</th>        </tr>        </thead>        <tbody class="table-dark" style="color: orangered;">        <%            for (AdminMsg message: messages)            {        %>            <tr>                <td><%=message.getName()%></td>                <td><%=message.getEmail()%></td>                <td><%=message.getMsg()%></td>                <td><a href="msgdel/<%=message.getMsgId()%>"class="btn btn-outline-info">Delete</a></td>            </tr>        <%            }        %>        </tbody>    </table></div><!-- Optional JavaScript --><!-- jQuery first, then Popper.js, then Bootstrap JS --><script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script><script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script><script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script></body></html>